<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="258.0" prefWidth="420.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.SetPersonalRatingController">
   <children>
      <Label layoutX="82.0" layoutY="26.0" prefHeight="50.0" prefWidth="275.0" text="Assign personal rating to a movie" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Button fx:id="submitBtn" layoutX="46.0" layoutY="218.0" mnemonicParsing="false" onAction="#submitBtn" text="Submit" />
      <Button fx:id="closeButton" layoutX="334.0" layoutY="218.0" mnemonicParsing="false" onAction="#close" text="Close" />
      <ComboBox fx:id="ratingBox" layoutX="228.0" layoutY="116.0" prefWidth="150.0" promptText="Ratings" />
      <ComboBox fx:id="moviesBox" layoutX="46.0" layoutY="116.0" prefWidth="150.0" promptText="Movies" />
   </children>
</AnchorPane>
